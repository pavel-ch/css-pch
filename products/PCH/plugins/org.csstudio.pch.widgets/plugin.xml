            category="ANDRITZ"
            description="A widget that displays 0/1 values as colored rectangle with a label. "
            editpart_class="org.csstudio.pch.widgets.FaultIndicatorEditPart"
            icon="platform:/plugin/org.csstudio.pch.widgets/icons/FaultIndicator.png"
            model_class="org.csstudio.pch.widgets.FaultIndicatorModel"
            name="FaultIndicator"
            typeId="org.csstudio.pch.widgets.FaultIndicator">
      </widget>
   </extension>
