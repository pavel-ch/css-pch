<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.csstudio.opibuilder.widget">
      <widget
            category="ANDRITZ"
            description="A widget that displays 0/1/2/3 values as colored labels. "
            editpart_class="org.csstudio.pch.widgets.BinaryEditPart"
            icon="platform:/plugin/org.csstudio.pch.widgets/icons/Binary.png"
            model_class="org.csstudio.pch.widgets.BinaryModel"
            name="Binary"
            typeId="org.csstudio.pch.widgets.Binary">
      </widget>
